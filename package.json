  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pasante-bot/core.git"
  },
  "keywords": [
    "Pasante",
    "Bot",
    "Telegram",
    "Discord",
    "Telegram Bot",
    "Discord Bot"
  ],
  "author": "Pasante-bot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pasante-bot/core/issues"
  },
  "homepage": "https://github.com/Pasante-bot/core#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
